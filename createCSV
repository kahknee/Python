# Creating/opening a new file name and its access mode
fileName = "newFile.txt"
accessMode = "w"
file = open(fileName, accessMode)

# Asking user for names and ages to write into file
for n in range(5):
    name = input("Name: ")
    age = input("Age: ")
    file.write(name + ", " + age + "\n")

file.close()
